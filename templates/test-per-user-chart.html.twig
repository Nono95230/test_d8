
<div id="test_d8-charts">
  <div id="tabs">
    <ul>
{% for id, arr in data.tabs %}
      <li><a href="#tabs-{{ id }}">{{ arr['name'] }} <small>({{ arr['num'] }})</small></a></li>
{% endfor %}
    </ul>

{% for id, arr in data.tabs %}
    <div id="tabs-{{ id }}">
      <!--div id="chart-{{ id }}"></div-->
      <canvas id="chart-{{ id }}" style="width:100%;height:250px;"></canvas>
      <div class="progress"><div class="progress-bar" role="progressbar" style="width:75%" aria-valuenow="75" aria-valuemin="0" aria-valuemax="40"></div></div>
    </div>
{% endfor %}
  </div>
</div>

